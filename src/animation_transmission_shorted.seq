#!/bin/bash


# "Concatenating" all selected questions/answers/notes into a single TeX file.
# This is where you specify the questions you want included in your worksheet, in the
# order you want them to appear.
cat \
04086.tex \
> all.tex


echo "Creating GIF animation of a shorted transmission line (file #04086)"
convert -delay 50 04086x*eps animation_transmission_shorted_fast.gif
convert -delay 250 04086x*eps animation_transmission_shorted_slow.gif

mv -v animation*gif ../output


